{
  "expo": {
    "name": "Pocket",
    "slug": "Pocket",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#CC0000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "scheme": "asa",
    "android": {
      "package": "com.appstoreafrica.pocketnomade",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#CC0000"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "privacy":"public"
  }
}
